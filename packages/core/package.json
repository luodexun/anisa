{
  "name": "@luodexun/core",
  "description": "start of core",
  "version": "0.0.19",
  "author": "luodexun",
  "bin": {
    "core": "./bin/run"
  },
  "bugs": "https://github.com/luodexun/anisa/issues",
  "dependencies": {
    "@arkecosystem/core-container": "^2.5.38",
    "@arkecosystem/core-interfaces": "^2.5.38",
    "@arkecosystem/crypto": "^2.5.38",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@typeskrift/foreman": "^0.2.1",
    "cli-table3": "^0.5.1",
    "cross-env": "^7.0.0",
    "env-paths": "^2.2.0",
    "envfile": "^4.5.0",
    "latest-version": "^5.1.0",
    "prompts": "^2.3.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/luodexun/anisa.git",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "core",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./lib/hooks/init/config",
      "command_not_found": "./lib/hooks/command_not_found/suggest"
    }
  },
  "repository": "https://github.com/luodexun/anisa.git",
  "scripts": {
    "anisa": "./bin/run",
    "postpack": "rm -f oclif.manifest.json",
    "clean": "del lib",
    "compile": "../../node_modules/typescript/bin/tsc",
    "build": "yarn clean && yarn compile",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "start:testnet": "yarn build && cross-env CORE_PATH_CONFIG=./bin/config/testnet  yarn anisa core:start  --env=test"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "lib/index.d.ts",
  "gitHead": "786ccd6bbd0d552d403d007b251244fe404dd606"
}
